<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DefaultProgressDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="progressDrawable">
        <item name="android:indeterminateDrawable">@drawable/centa_progress</item>
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateDuration">2000</item>
    </style>
</resources>